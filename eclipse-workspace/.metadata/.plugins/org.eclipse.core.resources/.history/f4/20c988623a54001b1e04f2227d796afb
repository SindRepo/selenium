package org.test;

import java.util.LinkedHashMap;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;

public class Repetive_Program {
	
	public static void main(String[] args) {
		
	//	String sentence = "For the people by the people of the people";
		
		Map<String, Integer> linkedmap = new LinkedHashMap<>();
		
	//	linkedmap.put(sentence, 1);
		
	//	System.out.println(sentence);
		
		linkedmap.put("For", 0);
		linkedmap.put("the", 1);
		linkedmap.put("people", 2);
		linkedmap.put("by", 3);
		
		System.out.println(linkedmap);
		
		
		linkedmap.get(2);
		
		System.out.println(linkedmap);
		
		boolean key = linkedmap.containsKey("for");
		
		System.out.println(key);
		
		Set<Entry<String, Integer>> entrySet = linkedmap.entrySet();
		
		System.out.println(entrySet);
		
		Set<String> keySet = linkedmap.keySet();
		
		System.out.println(keySet);
		
		
		
	}

}
